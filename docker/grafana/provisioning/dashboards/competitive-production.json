{
  "id": null,
  "uid": "jackdaw-competitive-production",
  "title": "Jackdaw Sentry - Production Competitive Dashboard",
  "tags": ["competitive", "production", "jackdaw", "blockchain"],
  "timezone": "browser",
  "schemaVersion": 39,
  "panels": [
    {
      "id": 1,
      "title": "Overall Competitive Parity",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "competitive_overall_parity",
          "legendFormat": "Parity %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"color": "red", "value": 70},
              {"color": "yellow", "value": 80},
              {"color": "green", "value": 90}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Market Position",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "competitive_market_position_score",
          "legendFormat": "Position Score"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "mappings": [
            {
              "options": {
                "0": {"text": "Market Leader", "color": "green", "index": 0},
                "1": {"text": "Strong Competitor", "color": "yellow", "index": 1},
                "2": {"text": "Emerging Player", "color": "red", "index": 2}
              },
              "type": "value"
            }
          ]
        }
      }
    },
    {
      "id": 8,
      "title": "System Health",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 16},
      "collapsed": false
    },
    {
      "id": 9,
      "title": "API Health",
      "type": "stat",
      "gridPos": {"h": 4, "w": 8, "x": 0, "y": 17},
      "targets": [
        {
          "expr": "up{job=\"api\"}",
          "legendFormat": "API Status"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": {"text": "Down", "color": "red", "index": 0},
                "1": {"text": "Up", "color": "green", "index": 1}
              },
              "type": "value"
            }
          ]
        }
      }
    }
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "refresh": "5m"
}
