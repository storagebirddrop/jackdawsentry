{
  "dashboard": {
    "id": null,
    "title": "Jackdaw Sentry - Production Competitive Dashboard",
    "tags": ["competitive", "production", "jackdaw", "blockchain"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Overall Competitive Parity",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "competitive_overall_parity",
            "legendFormat": "Parity %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 70},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 90}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 2,
        "title": "Market Position",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "competitive_market_position_score",
            "legendFormat": "Position Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"color": "red", "index": 0},
                  "Market Leader": {"color": "green", "index": 1},
                  "Strong Competitor": {"color": "blue", "index": 2},
                  "Viable Alternative": {"color": "yellow", "index": 3},
                  "Needs Improvement": {"color": "red", "index": 4}
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 3,
        "title": "Performance Trends",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "competitive_graph_expansion_parity",
            "legendFormat": "Graph Expansion"
          },
          {
            "expr": "competitive_api_parity",
            "legendFormat": "API Performance"
          },
          {
            "expr": "competitive_pattern_parity",
            "legendFormat": "Pattern Detection"
          },
          {
            "expr": "competitive_scalability_parity",
            "legendFormat": "Scalability"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 70},
                {"color": "yellow", "value": 85},
                {"color": "green", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Anomaly Detection",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 17},
        "targets": [
          {
            "expr": "competitive_anomalies",
            "legendFormat": "Anomalies"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {}
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Cost Analysis",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 25},
        "targets": [
          {
            "expr": "competitive_jackdaw_monthly_cost",
            "legendFormat": "Jackdaw Sentry"
          },
          {
            "expr": "competitive_market_average_cost",
            "legendFormat": "Market Average"
          },
          {
            "expr": "competitive_premium_leader_cost",
            "legendFormat": "Premium Leader"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "USD"
          }
        }
      },
      {
        "id": 6,
        "title": "ROI Analysis",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 34},
        "targets": [
          {
            "expr": "competitive_roi_3_year",
            "legendFormat": "3-Year ROI"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            }
        }
      },
      {
        "id": 7,
        "title": "Active Alerts",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 34},
        "targets": [
          {
            "expr": "competitive_active_alerts",
            "legendFormat": "Active Alerts"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "System Health",
        "type": "row",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 42},
        "panels": [
          {
            "id": 9,
            "title": "API Health",
            "type": "stat",
            "gridPos": {"h": 4, "w": 8, "x": 0, "y": 0},
            "targets": [
              {
                "expr": "up{job=\"api\"}",
                "legendFormat": "API Status"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "mappings": [
                  {
                    "options": {
                      "0": {"color": "red", "index": 0},
                      "1": {"color": "green", "index": 1}
                    },
                    "type": "value"
                  }
                ]
              }
            }
          },
          {
            "id": 10,
            "title": "Dashboard Health",
            "type": "stat",
            "gridPos": {"h": 4, "w": 8, "x": 8, "y": 0},
            "targets": [
              {
                "expr": "up{job=\"competitive-dashboard\"}",
                "legendFormat": "Dashboard Status"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "mappings": [
                  {
                    "options": {
                      "0": {"color": "red", "index": 0},
                      "1": {"color": "green", "index": 1}
                    },
                    "type": "value"
                  }
                ]
              }
            }
          },
          {
            "id": 11,
            "title": "Database Health",
            "type": "stat",
            "gridPos": {"h": 4, "w": 8, "x": 16, "y": 0},
            "targets": [
              {
                "expr": "up{job=\"postgres\"}",
                "legendFormat": "Database Status"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "mappings": [
                  {
                    "options": {
                      "0": {"color": "red", "index": 0},
                      "1": {"color": "green", "index": 1}
                    },
                    "type": "value"
                  }
                ]
              }
            }
          }
        ]
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "5m"
  }
}
